  MDL2B : SUBROUTINE

  ! match DL2 dogleg dispersion

  USE, DL23
  VALUE, KQDL
  MATCH, BETA0=TWSSM
    VARY, KQDL, STEP=1.E-5, UPPER=1.04, LOWER=0
    RMATRIX, #S/#E, RM(1,6)=0, RM(2,6)=0
    TMATRIX, #S/#E, TM(5,6,6)=0, WEIGHT(5,6,6)=0
    !LMDIF
    !MIGRAD
