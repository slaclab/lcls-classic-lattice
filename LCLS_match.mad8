  MUNDM : SUBROUTINE

  ! match betas at entrance to undulator

  USE, LTU
  VALUE, KQUM1, KQUM2, KQUM3, KQUM4
  MATCH, ENERGY=Ef, BETX=MBETX, ALFX=MALFX, BETY=MBETY, ALFY=MALFY
